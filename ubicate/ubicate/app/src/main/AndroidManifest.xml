            </intent-filter>
        </activity>
        <activity android:name=".MainActivity2"></activity>
        <activity android:name=".MainActivity3"></activity>
        <activity android:name=".MainActivity4"></activity>
        <activity android:name=".MainActivity5"></activity>
        <activity android:name=".Pop0"></activity>
        <activity android:name=".Pop1"></activity>
        <activity android:name=".Pop2"></activity>
        <activity android:name=".Pop3"></activity>
        <activity android:name=".Pop4"></activity>
        <activity android:name=".Pop5"></activity>
